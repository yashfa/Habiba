@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ViewMembers</title>
    @Html.Partial("PartialPage")
   
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
</head>

<body style="font-family:Verdana ;color:white;overflow-x:hidden ;background-image:url('/Content/images/nbg16.jpg');background-repeat:no-repeat;background-size:cover;">
    <div class="container-fluid">

        @Html.Partial("AdminPartialHeader")

        <h2>List of all Members <br /> <br /> </h2>

        <div class="table-responsive">
            <table class="table" style="background: #145214; padding: 10px; width: 95%; border-radius: 10px; margin-left: 30px">

                <tr>
                    <td style="padding-left:10px"><br />&nbsp; ID <br /><br /></td>
                    <td><br />&nbsp; Name <br /><br /></td>
                    <td><br />&nbsp; Username <br /><br /></td>
                    <td><br />&nbsp; Password <br /><br /></td>
                    <td><br /> &nbsp; Address <br /><br /> </td>
                    <td><br /> &nbsp; Cnic <br /><br /> </td>
                    <td><br /> &nbsp; Role <br /><br /> </td>

                </tr>

                <tbody>

                    @foreach (var s in Model)
                    {
                        <tr style="border:1px solid black">

                            <td style="padding:10px;padding-left:10px"> @s.user_id</td>
                            <td style="padding:10px"> @s.name</td>
                            <td style="padding:10px"> @s.username</td>
                            <td style="padding:10px"> @s.password</td>
                            <td style="padding:10px"> @s.address</td>
                            <td style="padding:10px"> @s.cnic</td>
                            <td style="padding:10px"> @s.role</td>

                            <td>
                                &nbsp; &nbsp; &nbsp; <a href="/Admin/Delete1/@s.user_id">   Delete </a>
                                &nbsp; &nbsp; &nbsp;
                            </td>
                        </tr>
                    }


                </tbody>
            </table>
        </div>
        <!---->
        <br /><br />
    </div>





    <script src="~/js/bootstrap.js" type="text/javascript"></script>

</body>

</html>
